@model List<Monthley.Models.MonthModels.PieSliceModel>

@{
    var chart = new Chart(width: 750, height: 750, theme: ChartTheme.Green)
   .AddTitle("Expected Income")
   .AddSeries("Default", chartType: "Pie",
        xValue: Model, xField: "Label",
        yValues: Model, yFields: "Amount").AddLegend().Write();
}
